package biblioteca;

import java.util.Scanner;

public class classe {

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		
		int numero = 0;
		int contador = 0;
		int soma = 0;
		
		System.out.print("Digite um numero de 1 a 9: ");
		numero = ler.nextInt();

		if (numero < 1)
		{
			System.out.println("O número é invalido!");
		}
		if (numero >9)
		{
			System.out.println("O número é invalido!");
		}
		do contador++
		{
			soma = soma + contador;
		}
		while (contador > numero);
			
		
		
	}

}
